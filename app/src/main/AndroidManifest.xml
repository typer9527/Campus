<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yl.campus">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/NoActionBarTheme">
        <activity android:name=".app.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.activity.NewsActivity" />
        <activity android:name=".app.activity.NewsContentActivity" />
        <activity android:name=".app.activity.LoginActivity" />
        <activity android:name=".app.activity.PersonalInfoActivity" />
        <activity android:name=".app.activity.CurriculumActivity" />
        <activity android:name=".app.activity.BookSearchActivity" />
        <activity android:name=".app.activity.SettingActivity"></activity>
    </application>

</manifest>