<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yl.campus">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/NoActionBarTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".app.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".app.activity.MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".app.activity.NewsActivity" />
        <activity android:name=".app.activity.NewsContentActivity" />
        <activity android:name=".app.activity.LoginActivity" />
        <activity android:name=".app.activity.PersonalInfoActivity" />
        <activity android:name=".app.activity.CurriculumActivity" />
        <activity android:name=".app.activity.BookSearchActivity" />
        <activity android:name=".app.activity.SettingActivity" />
        <activity android:name=".app.activity.PayModeActivity" />
        <activity android:name=".app.activity.BalanceActivity"></activity>
    </application>

</manifest>